{
    "name": "ishanvyas22/cakephp-inertiajs",
    "description": "The CakePHP adapter for Inertia.js.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": ">=3.5.0 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Inertia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inertia\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
